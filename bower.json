{
  "name": "patternfly-sass",
  "version": "3.8.1",
  "license": "Apache-2.0",
  "homepage": "https://www.patternfly.org",
  "authors": [
    "Alex Wood",
    "Dávid Halász"
  ],
  "description": "Red Hat's Patternfly, converted to Sass and ready to drop into Rails",
  "reposityory": {
    "type": "git",
    "url": "git://github.com/patternfly/patternfly-sass"
  },
  "moduleType": "globals",
  "main": [
    "assets/stylesheets/_patternfly.scss",
    "assets/javascripts/patternfly.js"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "lib",
    "tasks",
    "pkg",
    "bower_components",
    "spec",
    "tmp"
  ],
  "dependencies": {
    "bootstrap-sass": "~3.3.7",
    "bootstrap-combobox": "~1.1.7",
    "bootstrap-datepicker": "~1.6.4",
    "bootstrap-select": "~1.10.0",
    "bootstrap-switch": "~3.3.2",
    "bootstrap-touchspin": "~3.1.1",
    "c3": "~0.4.11",
    "datatables": "~1.10.12",
    "datatables-colreorder": "~1.3.2",
    "datatables-colvis": "~1.1.2",
    "font-awesome-sass": "~4.6.2",
    "google-code-prettify": "~1.0.4",
    "jquery": "~2.1.4",
    "matchHeight": "~0.7.0",
    "eonasdan-bootstrap-datetimepicker": "~4.17.37",
    "moment": "~2.14.1",
    "patternfly-bootstrap-treeview": "~1.0.0"
  }
}
